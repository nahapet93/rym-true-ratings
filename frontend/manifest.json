{
  "name": "RateYourMusic True Ratings",
  "description": "RateYourMusic album ratings based on song ratings",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["scripting", "storage"],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "action": {
    "default_title": "Click Me",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://*.rateyourmusic.com/*"],
      "js": ["script.js"]
    }
  ]
}
